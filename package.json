{
  "name": "learn-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "crossenv": "cnpm install cross-env --save-dev",
    "server": "cnpm install --save-dev webpack-dev-server",
    "start": "cross-env NODE_ENV=development && webpack-dev-server --progress",
    "build": "cross-env NODE_ENV=production && webpack --config ./webpack.production.config.js --progress",
    "babel": "cnpm install babel-loader babel-core babel-preset-es2015 --save-dev",
    "babelreact": "cnpm install babel-preset-react --save-dev",
    "json": "cnpm install --save-dev json-loader",
    "react": "cnpm install --save react react-dom",
    "css": "cnpm install --save-dev style-loader css-loader",
    "autoprefixer": "cnpm install --save-dev autoprefixer",
    "postcss": "cnpm install --save-dev postcss-loader",
    "html": "cnpm install --save-dev html-webpack-plugin",
    "reacthmr": "cnpm install --save-dev babel-plugin-react-transform react-transform-hmr",
    "extract": "cnpm install --save-dev extract-text-webpack-plugin",
    "sass": "cnpm install sass-loader node-sass --save-dev ",
    "clean": "cnpm install clean-webpack-plugin --save-dev",
    "jquery": "cnpm install --save jquery",
    "md5": "cnpm install webpack-md5-hash --save-dev",
    "file": "cnpm install --save-dev file-loader",
    "url": "cnpm install --save-dev url-loader",
    "reacthot": "cnpm install --save react-hot-loader",
    "dll": "webpack --config ./webpack.dll.config.js --progress",
    "copy": "cnpm install --save-dev copy-webpack-plugin",
    "classname": "cnpm install --save classnames",
    "notifier": "cnpm install --save-dev webpack-notifier"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.2.5",
    "jquery": "^3.2.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^3.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.2",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack-dev-server": "^2.6.1",
    "webpack-notifier": "^1.5.0"
  }
}
